<h1>Editing folder</h1>

<%= render 'form' %>

<h3>content</h3>
<%= render 'content_bookmarks' %>
<script type="text/javascript">
    $('#bookmark_container a').on('ajax:success', function(){
        $.get('<%= content_of_folder_path %>', {}, function (data){
            $('#bookmark_container').html(data);
        });
    });
</script>
<p>
<%= link_to 'Show', @folder %> |
<%= link_to 'Back', folders_path %></p>
